/*This is Myaccount/index css file!*/
*{
    margin: 0;
    padding: 0;
}

#wrapper {
    position:absolute; z-index:1;
    top:0px;
    bottom:0px;
    left:0px;
    width:100%;
    background:#eee;
    overflow:auto;
}

#scroller {
    position:absolute;
    z-index:1;
    /*	-webkit-touch-callout:none;*/
    -webkit-tap-highlight-color:rgba(0,0,0,0);
    width:100%;
    padding:0;
}

#scroller ul {
    list-style:none;
    padding:0;
    margin:0;
    width:100%;
    text-align:left;
}
.integral{
	width:100%;
	padding:1em 0;
	border-bottom:1px solid #eee;
	background:#fff;
}
.integral_round{
	width:6em;
	height:6em;
	background:#1AAD16;
	border-radius:50%;
	margin:0 auto;
	text-align: center;
	color:#fff;
}
.integral_number{
	padding-top:1.9em;
	font-size:.8em;
}
.integral_recharge{
	color:#fff;
}
.integral_title{
	color:#999;
	font-size:.8em;
}
.record_time{
	color:#999;
	font-size:.8em;
}
#myFrame {
    position:absolute;
    top:0; left:0;
}
#pullDown, #pullUp {
    background:#fff;
    height:40px;
    line-height:40px;
    padding:5px 10px;
    border-bottom:1px solid #eee;
    font-size:14px;
    color:#888;
    text-align: center;
}
#pullUp{
    border-top:1px solid #eee;
    /*display:none;*/
}

@-webkit-keyframes loading {
    from { -webkit-transform:rotate(0deg) translateZ(0); }
    to { -webkit-transform:rotate(360deg) translateZ(0); }
}
